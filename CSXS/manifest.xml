	<ExtensionList>

		<Extension Id="sergey.kupin.examples.dd" Version="1.0.0" />

	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="ILST" Version="19.2" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="6.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		


		<Extension Id="sergey.kupin.examples.dd">
